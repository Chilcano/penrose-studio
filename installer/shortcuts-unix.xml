<shortcuts>
	<skipIfNotSupported/>
	<programGroup defaultName="Penrose Studio"
		location="applications"/>
	<shortcut name="Penrose Studio"
		description="Penrose Studio"
		programGroup="yes" 
		applications="yes" 
		target="$INSTALL_PATH/penrose-studio"
		workingDirectory="$INSTALL_PATH"
		type="Application"
		encoding="UTF-8"
		terminal="false"
		initialState="normal">
		<createForPack name="Penrose Studio Executables"/>
	</shortcut>
	<shortcut name="Uninstall Penrose Studio"
		description="Uninstall Penrose Studio"
		programGroup="yes"
		target="$JAVA_HOME/bin/java"
		commandLine="-jar &quot;$INSTALL_PATH/Uninstaller/uninstaller.jar&quot;"
		iconFile="$INSTALL_PATH/uninstall.png"
		iconIndex="0"
		initialState="noShow"
		workingDirectory=""
		type="Application"
		encoding="UTF-8">
		<createForPack name="Penrose Studio Executables"/>
	</shortcut>
</shortcuts>
